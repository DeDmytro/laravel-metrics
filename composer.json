{
  "name": "dedmytro/laravel-metrics",
  "description": "Provides customizable dashboard to see project metrics",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "laravel metrics",
    "laravel dashboard",
    "laravel analytics",
    "laravel request per second",
    "laravel users online"
  ],
  "authors": [
    {
      "name": "Hodun Dmytro",
      "email": "godundmytro@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/DeDmytro/laravel-metrics/issues",
    "source": "https://github.com/DeDmytro/laravel-metrics"
  },
  "require": {
    "php": "^7.4",
    "illuminate/support": "^8.0",
    "ext-pdo": "*"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "DeDmytro\\Metrics\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "DeDmytro\\Metrics\\MetricsServiceProvider"
      ]
    }
  }
}
